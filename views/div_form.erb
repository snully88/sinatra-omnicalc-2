<h1>New division</h1>

<form action="/wizard_divide">
  <div>
    <label for="form_first_number">
      Divide this:
    </label>

    <input id="form_first_number" name="first_number" text="text">
  </div>

  <div>
    <label for="form_second_number">
      by this:
    </label>
    <input id="form_second_number" name="second_number" text="text">
  </div>

  <button>
    Divide!
  </button>
</form>
